{
	"name": "vite-react-template",
	"description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
	"private": true,
	"scripts": {
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types",
		"check": "tsc && vite build && wrangler deploy --dry-run",
		"deploy": "wrangler deploy",
		"deploy:uat": "npm run build && rm -rf .wrangler/deploy && wrangler deploy --env uat --minify",
		"deploy:pro": "npm run build && rm -rf .wrangler/deploy && wrangler deploy --env production --minify",
		"dev": "wrangler dev --port 5173",
		"dev:local": "wrangler dev --port 5173 --local",
		"dev:remote": "wrangler dev --port 5173 --remote",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 execute DB --local --file=./migrations/0001_*.sql",
		"db:migrate:uat": "wrangler d1 execute DB --env uat --file=./migrations/0001_*.sql",
		"db:migrate:pro": "wrangler d1 execute DB --env production --file=./migrations/0001_*.sql",
		"db:studio": "drizzle-kit studio",
		"db:studio:local": "drizzle-kit studio --config drizzle.config.local.ts",
		"db:studio:remote": "drizzle-kit studio --config drizzle.config.ts"
	},
	"cloudflare": {
		"label": "Vite React Template",
		"products": [
			"Workers"
		],
		"categories": [],
		"preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/532a5378-9717-4964-385b-5fafd2160200/public",
		"preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/ec7f4be5-d1d6-4e94-0851-126341745400/preview",
		"publish": true
	},
	"dependencies": {
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-slot": "^1.2.4",
		"@react-three/drei": "^10.7.7",
		"@react-three/fiber": "^9.4.0",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/react-router": "^1.136.18",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.7",
		"hono": "4.8.2",
		"lucide-react": "^0.554.0",
		"motion": "^12.23.24",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"three": "^0.181.2"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.15.1",
		"@eslint/js": "9.29.0",
		"@libsql/client": "^0.15.15",
		"@tanstack/router-devtools": "^1.136.18",
		"@tanstack/router-plugin": "^1.136.18",
		"@types/node": "^24.0.4",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"@types/three": "^0.181.0",
		"@vitejs/plugin-react": "5.0.0",
		"drizzle-kit": "^0.31.7",
		"eslint": "9.27.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"globals": "16.2.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.0",
		"vite": "^6.0.0",
		"wrangler": "4.49.1"
	},
	"type": "module"
}
