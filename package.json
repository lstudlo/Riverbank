{
	"name": "riverbank",
	"description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types",
		"check": "tsc && vite build && wrangler deploy --dry-run",
		"deploy": "wrangler deploy",
		"deploy:uat": "npm run build && rm -rf .wrangler/deploy && wrangler deploy --env uat --minify",
		"deploy:pro": "npm run build && rm -rf .wrangler/deploy && wrangler deploy --env pro --minify",
		"dev": "vite --port 5173",
		"dev:local": "wrangler dev --port 5173 --local",
		"dev:remote": "wrangler dev --port 5173 --remote",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply DB --local",
		"db:migrate:uat": "wrangler d1 migrations apply DB --env uat --remote",
		"db:migrate:pro": "wrangler d1 migrations apply DB --env pro --remote",
		"db:migrations:list": "wrangler d1 migrations list DB --local",
		"db:migrations:list:uat": "wrangler d1 migrations list DB --env uat --remote",
		"db:migrations:list:pro": "wrangler d1 migrations list DB --env pro --remote",
		"db:studio": "drizzle-kit studio --config drizzle.config.local.ts",
		"db:studio:uat": "drizzle-kit studio --config drizzle.config.uat.ts",
		"db:studio:pro": "drizzle-kit studio --config drizzle.config.pro.ts"
	},
	"cloudflare": {
		"label": "riverbank",
		"products": [
			"Workers"
		],
		"categories": [],
		"preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/532a5378-9717-4964-385b-5fafd2160200/public",
		"preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/ec7f4be5-d1d6-4e94-0851-126341745400/preview",
		"publish": true
	},
